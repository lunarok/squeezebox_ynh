{
  "name": "Squeezebox",
  "id": "squeezebox",
  "description": {
    "en": "Multiroom audio diffusion",
    "fr": "Diffusion audio multiroom"
  },
  "developer": {
    "name": "lunarok",
    "email": "cedric@wayang.fr",
    "url": "http://www.wayang.fr"
  },
  "multi_instance": "false",
  "arguments": {
    "install": [
      {
        "name": "domain",
        "ask": {
          "en": "Choose a domain for Squeezebox"
        },
        "example": "domain.org"
      },
      {
        "name": "path",
        "ask": {
          "en": "Choose a path for Squeezebox"
        },
        "example": "/squeezebox",
        "default": "/squeezebox"
      },
      {
        "name": "server",
        "ask": {
          "en": "Choose IP where Squeezebox is installed"
        },
        "example": "127.0.0.1",
        "default": "127.0.0.1"
      }
    ]
  }
}
